<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="saas_plans_view_form" model="ir.ui.view">
        <field name="name">saas_plans.view.form</field>
        <field name="model">saas_plans</field>
        <field name="arch" type="xml">
            <form string="Planning">
                <sheet>
                    <h1>
                        <field name="name"></field>
                    </h1>
                </sheet>
            </form>
        </field>
    </record>

    <record id="saas_plans_view_tree" model="ir.ui.view">
        <field name="name">saas_plans.view.tree</field>
        <field name="model">saas_plans</field>
        <field name="arch" type="xml">
            <tree string="Planning List">
                <field name="name"></field>
            </tree>
        </field>
    </record>

    <record id="saas_plans_action" model="ir.actions.act_window">
        <field name="name">Planning</field>
        <field name="res_model">saas_plans</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
